{
  "name": "xchange",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.7.3",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "dotenv": "^6.0.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.5",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.5.1",
    "react-app-rewire-less": "^2.1.2",
    "react-app-rewire-mobx": "^1.0.8",
    "react-app-rewire-webpack-bundle-analyzer": "^1.0.1",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.5.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.4",
    "react-testing-library": "^5.1.0",
    "webpack-bundle-analyzer": "^3.0.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/sass/ -o src/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/sass/ -o src/css --watch --recursive",
    "start-js": "react-app-rewired start dotenv_config_path=.env",
    "build-js": "react-app-rewired build dotenv_config_path=.env.production",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-app-rewired test --env=jsdom --watch",
    "test:coverage": "react-app-rewired test --env=jsdom --coverage",
    "eject": "react-app-rewired eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
